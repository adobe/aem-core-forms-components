<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title=""
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <accordion
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Basic"
                            granite:class="cmp-adaptiveform-datepicker__editdialogdatepicker"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            active="{Boolean}true"
                            selected="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <name
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Name"
                                    name="./name"
                                    required="{Boolean}true">
                                <granite:data
                                        jcr:primaryType="nt:unstructured"
                                        node-name-validation=""/>
                            </name>
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Title"
                                    name="./jcr:title"/>
                            <hideTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    name="./hideTitle"
                                    text="Hide title"
                                    value="true"/>
                            <placeholder
                                    jcr:primaryType="nt:unstructured"
                                    granite:class="cmp-adaptiveform-datepicker__placeholder"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Placeholder Text"
                                    name="./placeholderText"/>
                            <required
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    checked="{Boolean}false"
                                    fieldLabel="Required field"
                                    labelledBy="Mandatory"
                                    name="./mandatory"
                                    granite:class="cmp-adaptiveform-datepicker__required"
                                    text="Required field"
                                    value="true">
                            </required>
                            <mandatoryMessage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldDescription="Error message shown when required field left empty."
                                    granite:class="cmp-adaptiveform-datepicker__mandatorymessage"
                                    fieldLabel="Required Field Message"
                                    name="./mandatoryMessage"/>
                            <validateExpMessage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Displayed when validation script fails."
                                    fieldLabel="Script Validation Message"
                                    name="./validateExpMessage"/>
                            <minimum
                                    jcr:primaryType="nt:unstructured"
                                    sling:orderBefore="guideGlobalLazyField"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Please enter the date in the required format  &quot;yyyy-mm-dd&quot;."
                                    fieldLabel="Minimum Value"
                                    emptyText="YYYY-MM-DD"
                                    name="./minimum"/>
                            <exclusiveMinimum
                                    jcr:primaryType="nt:unstructured"
                                    sling:orderBefore="guideGlobalLazyField"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    value="true"
                                    text="Exclude minimum value"
                                    fieldLabel="Exclusive Minimum"
                                    name="./exclusiveMinimum"/>
                            <maximum
                                    jcr:primaryType="nt:unstructured"
                                    sling:orderBefore="guideGlobalLazyField"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Please enter the date in the required format  &quot;yyyy-mm-dd&quot;."
                                    fieldLabel="Maximum Value"
                                    emptyText="YYYY-MM-DD"
                                    name="./maximum"/>
                            <exclusiveMaximum
                                    jcr:primaryType="nt:unstructured"
                                    sling:orderBefore="guideGlobalLazyField"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    text="Exclude maximum value"
                                    fieldLabel="Exclusive Maximum"
                                    value="true"
                                    name="./exclusiveMaximum"/>
                            <bindref
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/authoring/components/granite/form/bindrefselector"
                                    emptyText="Data Model Bind Reference"
                                    fieldDescription="A valid bind reference can have  alphanumeric characters, parenthesis, dot, hyphen, at the rate, forward slash, asterisk, dollar and underscore."
                                    fieldLabel="Data Model Bind Reference"
                                    name="./bindRef">
                                <granite:data
                                        jcr:primaryType="nt:unstructured"
                                        node-bindref-validation=""
                                        bindref-datamodel="fd.channelcore.dataObjectsTree.getDataModel"/>
                            </bindref>
                            <visible
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    granite:class="cmp-adaptiveform-datepicker__visible"
                                    name="./visible"
                                    text="Hide Object"
                                    uncheckedValue="true"
                                    value="false">
                            </visible>
                            <visible-typehint
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                    name="./visible@TypeHint"
                                    value="Boolean"/>
                            <enabled
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    granite:class="cmp-adaptiveform-datepicker__enabled"
                                    name="./enabled"
                                    text="Disable Object"
                                    uncheckedValue="true"
                                    value="false">
                            </enabled>
                            <enabled-typehint
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                    name="./enabled@TypeHint"
                                    value="Boolean"/>
                        </items>
                        <parentConfig
                                jcr:primaryType="nt:unstructured"
                                active="{Boolean}true"/>
                    </basic>
                    <help
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Help Content"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                    fieldLabel="Long Description"
                                    name="./longDescription"
                                    useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <links jcr:primaryType="nt:unstructured">
                                        <linkDialogConfig
                                                jcr:primaryType="nt:unstructured"
                                                height="{Long}316">
                                            <linkAttributes jcr:primaryType="cq:WidgetCollection"/>
                                        </linkDialogConfig>
                                    </links>
                                    <format
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <lists
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <justify
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <keys
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <paraformat
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <misctools
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <fullscreen
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[format#bold,format#italic,format#underline,fullscreen#start]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                        ref="justify"/>
                                                <lists
                                                        jcr:primaryType="nt:unstructured"
                                                        items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                        ref="lists"/>
                                            </popovers>
                                        </inline>
                                        <fullscreen
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,-,links#modifylink,links#unlink,links#anchor,tracklinks#modifylinktracking,-,justify#justifyleft,justify#justifycenter,justify#justifyright,-,image#imageProps,-,lists#unordered,lists#ordered,lists#outdent,lists#indent,-,misctools#sourceedit,fullscreen#finish]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <styles
                                                        jcr:primaryType="nt:unstructured"
                                                        items="styles:getStyles:styles-pulldown"
                                                        ref="styles"/>
                                                <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        items="paraformat:getFormats:paraformat-pulldown"
                                                        ref="paraformat"/>
                                            </popovers>
                                        </fullscreen>
                                    </cui>
                                </uiSettings>
                            </description>
                        </items>
                    </help>
                    <accessibility
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Accessibility"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <assistPriority
                                    jcr:primaryType="nt:unstructured"
                                    granite:class="cmp-adaptiveform-datepicker__assistpriority"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    emptyText="Select"
                                    fieldLabel="Screen Reader Precedence"
                                    name="./assistPriority"
                                    value="custom">
                                <items jcr:primaryType="nt:unstructured">
                                    <empty
                                            jcr:primaryType="nt:unstructured"
                                            text=""
                                            value=""/>
                                    <customText
                                            jcr:primaryType="nt:unstructured"
                                            text="Custom Text"
                                            value="custom"/>
                                    <description
                                            jcr:primaryType="nt:unstructured"
                                            text="Description"
                                            value="description"/>
                                    <caption
                                            jcr:primaryType="nt:unstructured"
                                            text="Title"
                                            value="label"/>
                                    <name
                                            jcr:primaryType="nt:unstructured"
                                            text="Name"
                                            value="name"/>
                                    <none
                                            jcr:primaryType="nt:unstructured"
                                            text="None"
                                            value="none"/>
                                </items>
                            </assistPriority>
                            <customText
                                    jcr:primaryType="nt:unstructured"
                                    wrapperClass="cmp-adaptiveform-datepicker__assistpriority-customtext"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldLabel="Custom Text"
                                    name="./custom"/>
                        </items>
                    </accessibility>
                    <patterns
                            jcr:title="Patterns"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </patterns>
                </items>
            </accordion>
        </items>
    </content>
</jcr:root>
